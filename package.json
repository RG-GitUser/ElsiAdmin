{
  "name": "myapp",
  "description": "My Awesome Electron App",
  "author": "Your Name",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "./",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build:react": "vite build",
    "dev:electron": "concurrently \"npm run dev\" \"wait-on http-get://localhost:3000 && electron .\"",
    "build:electron": "npm run build:react && electron-builder",
    "build:win": "npm run build:react && electron-builder --win",
    "build:win:debug": "npm run build:react && cross-env DEBUG=electron-builder electron-builder --win",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "firebase": "^12.2.1",
    "jspdf": "^3.0.3",
    "node.js": "^0.0.1-security",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.1.2",
    "wait-on": "^9.0.0"
  },
  "build": {
    "appId": "com.example.myapp",
    "productName": "My App",
    "files": [
      "dist/",
      "electron/",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    }
  }
}
